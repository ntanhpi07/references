.run logon.txt;
drop table DL_IACT.@table;
drop table DL_IACT.@table_err;
drop table DL_IACT.@table_uv;

CREATE MULTISET TABLE DL_IACT.@table
     (
      GROUPDN VARCHAR(500) CHARACTER SET UNICODE NOT CASESPECIFIC,
      /* CN VARCHAR(400) CHARACTER SET UNICODE NOT CASESPECIFIC, */
      "MEMBER" VARCHAR(500) CHARACTER SET UNICODE NOT CASESPECIFIC,
      userid VARCHAR(200) CHARACTER SET UNICODE NOT CASESPECIFIC,
      objtype VARCHAR(400) CHARACTER SET UNICODE NOT CASESPECIFIC)
NO PRIMARY INDEX ;

set record vartext "~" trim both;

define
      GROUPDN (VARCHAR(500)) ,
      /* CN (VARCHAR(400)) , */
      MEMBER_nm (VARCHAR(500)) ,
      userid (VARCHAR(200)) ,
      objtype (VARCHAR(400)) 
	 file=@file;

begin loading dl_iact.@table
  errorfiles dl_iact.@table_err, dl_iact.@table_uv;
record 2;
insert into DL_IACT.@table
  values (
  :groupdn, 
  /* :cn, */
  :member_nm,
  :userid,
  :objtype
  );

  end loading;
  logoff;